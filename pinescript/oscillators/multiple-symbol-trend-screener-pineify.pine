p_scr_ma_distance(series float source, simple int length, simple string ma_type, series float price) =>
    value = p_ta_ma(source, length, ma_type)
    distance_percent = ((price - value) / price)* 100
    [distance_percent]

p_scr_ma_distance_display(series float value, simple color overbought_text_color, simple color overbought_background_color, simple color oversold_text_color, simple color oversold_background_color) =>
    text_color = value > 0 ? overbought_text_color : oversold_text_color
    background_color = value > 0 ? overbought_background_color : oversold_background_color
    [str.tostring(value, "#.##") + '%', text_color, background_color]
p_scr_aroon(simple int length) =>
    upper = 100 * (ta.highestbars(high, length + 1) + length)/length
    lower = 100 * (ta.lowestbars(low, length + 1) + length)/length
    [upper, lower]

p_scr_aroon_display(series float up, series float down, simple color bullish_text_color, simple color bullish_background_color, simple color bearish_text_color, simple color bearish_background_color, simple color neutral_text_color, simple color neutral_background_color) =>
    is_bullish = up > down
    is_bearish = down > up
    text_value = str.tostring(up, "#.##") + "/" + str.tostring(down, "#.##")
    text_color = is_bullish ? bullish_text_color : is_bearish ? bearish_text_color : neutral_text_color
    background_color = is_bullish ? bullish_background_color : is_bearish ? bearish_background_color : neutral_background_color
    [text_value, text_color, background_color]
p_scr_psar(simple float start, simple float increment, simple float max) =>
    psar_value = ta.sar(start, increment, max)
    long_signal = ta.crossover(close, psar_value)
    short_signal = ta.crossunder(close, psar_value)
    var int psar_state = 0
    psar_state := long_signal ? 1 : short_signal ? -1 : nz(psar_state[1], 0)
    psar_state := psar_state == 0 ? (close > psar_value ? 1 : close < psar_value ? -1 : 0) : psar_state
    bars_since_signal = nz(ta.barssince(long_signal or short_signal), 100000)
    [psar_state, bars_since_signal]

p_scr_psar_display(series int trend_state, series int bars_since_signal, simple string long_symbol, simple string short_symbol, simple string neutral_symbol, simple color long_text_color, simple color long_background_color, simple color short_text_color, simple color short_background_color, simple color neutral_text_color, simple color neutral_background_color) =>
    has_signal = not na(bars_since_signal) and bars_since_signal < 100000
    suffix = has_signal ? str.tostring(bars_since_signal) : "--"
    symbol = trend_state > 0 ? long_symbol : trend_state < 0 ? short_symbol : neutral_symbol
    text_value = symbol + suffix
    text_color = trend_state > 0 ? long_text_color : trend_state < 0 ? short_text_color : neutral_text_color
    background_color = trend_state > 0 ? long_background_color : trend_state < 0 ? short_background_color : neutral_background_color
    [text_value, text_color, background_color]
p_scr_adx_trend(simple int smoothing, simple int di_length, simple float level, simple string di_ma_type, simple string adx_ma_type) =>
    up = ta.change(high)
    down = -ta.change(low)
    plus_dm = na(up) ? na : (up > down and up > 0 ? up : 0)
    minus_dm = na(down) ? na : (down > up and down > 0 ? down : 0)
    tr = p_ta_ma(ta.tr(true), di_length, di_ma_type)
    tr_safe = tr == 0 ? 1 : tr
    plus_di = fixnan(100 * p_ta_ma(plus_dm, di_length, di_ma_type) / tr_safe)
    minus_di = fixnan(100 * p_ta_ma(minus_dm, di_length, di_ma_type) / tr_safe)
    di_sum = plus_di + minus_di
    dx = di_sum == 0 ? 0 : 100 * math.abs(plus_di - minus_di) / di_sum
    sig = p_ta_ma(dx, smoothing, adx_ma_type)
    sig_prev = sig[1]
    sig_prev2 = sig[2]
    adx_trend_up = plus_di > minus_di and sig >= sig_prev and sig < level and sig > minus_di
    adx_trend_up_strong = plus_di > minus_di and sig >= sig_prev and sig >= level
    adx_trend_up_falling = plus_di > minus_di and sig < sig_prev and sig >= level
    adx_trend_down = minus_di > plus_di and sig > sig_prev and sig < level and sig > plus_di
    adx_trend_down_strong = minus_di > plus_di and sig > sig_prev and sig >= level
    adx_trend_down_falling = minus_di > plus_di and sig <= sig_prev and sig >= level
    adx_kick = sig <= sig_prev and sig_prev >= sig_prev2 and sig >= level
    state = adx_trend_up ? 2 : adx_trend_up_strong ? 3 : adx_trend_up_falling ? 4 : adx_trend_down ? 5 : adx_trend_down_strong ? 6 : adx_trend_down_falling ? 7 : adx_kick ? 1 : 0
    [state]

p_scr_adx_trend_display(series int state, simple string no_signal_text, simple color no_signal_text_color, simple color no_signal_background_color, simple string kick_text, simple color kick_text_color, simple color kick_background_color, simple string uptrend_text, simple color uptrend_text_color, simple color uptrend_background_color, simple string uptrend_strong_text, simple color uptrend_strong_text_color, simple color uptrend_strong_background_color, simple string uptrend_falling_text, simple color uptrend_falling_text_color, simple color uptrend_falling_background_color, simple string downtrend_text, simple color downtrend_text_color, simple color downtrend_background_color, simple string downtrend_strong_text, simple color downtrend_strong_text_color, simple color downtrend_strong_background_color, simple string downtrend_falling_text, simple color downtrend_falling_text_color, simple color downtrend_falling_background_color) =>
    text_value = state == 1 ? kick_text : state == 2 ? uptrend_text : state == 3 ? uptrend_strong_text : state == 4 ? uptrend_falling_text : state == 5 ? downtrend_text : state == 6 ? downtrend_strong_text : state == 7 ? downtrend_falling_text : no_signal_text
    text_color = state == 1 ? kick_text_color : state == 2 ? uptrend_text_color : state == 3 ? uptrend_strong_text_color : state == 4 ? uptrend_falling_text_color : state == 5 ? downtrend_text_color : state == 6 ? downtrend_strong_text_color : state == 7 ? downtrend_falling_text_color : no_signal_text_color
    background_color = state == 1 ? kick_background_color : state == 2 ? uptrend_background_color : state == 3 ? uptrend_strong_background_color : state == 4 ? uptrend_falling_background_color : state == 5 ? downtrend_background_color : state == 6 ? downtrend_strong_background_color : state == 7 ? downtrend_falling_background_color : no_signal_background_color
    [text_value, text_color, background_color]
p_scr_supertrend(series float factor, simple int atrPeriod) =>
    [supertrend_value, direction] = ta.supertrend(factor, atrPeriod)
    [supertrend_value, direction]

p_scr_supertrend_display(series float supertrend_value, series float direction, simple string up_text, simple string down_text, simple color up_text_color, simple color up_background_color, simple color down_text_color, simple color down_background_color) =>
    is_uptrend = direction < 0
    text_value = is_uptrend ? up_text : down_text
    text_color = is_uptrend ? up_text_color : down_text_color
    background_color = is_uptrend ? up_background_color : down_background_color
    [text_value, text_color, background_color]
p_scr_kc(series float source, simple int length, simple float mult, simple bool useTrueRange) =>
    [upper, middle, lower] = ta.kc(source, length, mult, useTrueRange)
    [upper, middle, lower]

p_scr_kc_display(series float source, series float upper, series float middle, series float lower, simple color above_text_color, simple color above_background_color, simple color below_text_color, simple color below_background_color, simple color neutral_text_color, simple color neutral_background_color) =>
    is_na_values = na(upper) or na(middle) or na(lower)
    
    if is_na_values
        ["--", neutral_text_color, neutral_background_color]
    else
        position_relative = source > upper ? "Above" : source < lower ? "Below" : "Inside"
        text_color = source > upper ? above_text_color : source < lower ? below_text_color : neutral_text_color
        background_color = source > upper ? above_background_color : source < lower ? below_background_color : neutral_background_color
        [position_relative, text_color, background_color]
p_scr_bbtrend(simple int short_bb_length, simple int long_bb_length, simple float mult) =>
    [short_middle, short_upper, short_lower] = ta.bb(close, short_bb_length, mult)
    [long_middle,  long_upper,  long_lower]  = ta.bb(close, long_bb_length,  mult)
    bbtrend = (math.abs(short_lower - long_lower) - math.abs(short_upper - long_upper)) / short_middle * 100
    [bbtrend]

p_scr_bbtrend_display(series float bbtrend_value, simple color bullish_text_color, simple color bullish_background_color, simple color bearish_text_color, simple color bearish_background_color) =>
    text_value = str.tostring(bbtrend_value, "#.##")
    text_color = bbtrend_value > 0 ? bullish_text_color : bearish_text_color
    background_color = bbtrend_value > 0 ? bullish_background_color : bearish_background_color
    [text_value, text_color, background_color]

p_screener_indicator(series float p_scr_ind_1_source, simple int p_scr_ind_1_length, simple string p_scr_ind_1_maType, series float p_scr_ind_1_price, simple int p_scr_ind_2_length, simple float p_scr_ind_3_start, simple float p_scr_ind_3_increment, simple float p_scr_ind_3_max, simple int p_scr_ind_4_smoothing, simple int p_scr_ind_4_di_length, simple float p_scr_ind_4_level, simple string p_scr_ind_4_di_ma_type, simple string p_scr_ind_4_adx_ma_type, series float p_scr_ind_5_factor, simple int p_scr_ind_5_atrPeriod, series float p_scr_ind_6_source, simple int p_scr_ind_6_length, simple float p_scr_ind_6_mult, simple bool p_scr_ind_6_useTrueRange, simple int p_scr_ind_7_shortLength, simple int p_scr_ind_7_longLength, simple float p_scr_ind_7_multiplier) =>
    [p_scr_ind_1_value] = p_scr_ma_distance(p_scr_ind_1_source, p_scr_ind_1_length, p_scr_ind_1_maType, p_scr_ind_1_price)
    [p_scr_ind_2_up, p_scr_ind_2_down] = p_scr_aroon(p_scr_ind_2_length)
    [p_scr_ind_3_trend_state, p_scr_ind_3_bars_since_signal] = p_scr_psar(p_scr_ind_3_start, p_scr_ind_3_increment, p_scr_ind_3_max)
    [p_scr_ind_4_state] = p_scr_adx_trend(p_scr_ind_4_smoothing, p_scr_ind_4_di_length, p_scr_ind_4_level, p_scr_ind_4_di_ma_type, p_scr_ind_4_adx_ma_type)
    [p_scr_ind_5_supertrend, p_scr_ind_5_direction] = p_scr_supertrend(p_scr_ind_5_factor, p_scr_ind_5_atrPeriod)
