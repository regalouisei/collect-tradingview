//@version=5
indicator(title="HaP RSI Combo", shorttitle="HaP RSI Combo", overlay=true, max_bars_back=4000)

// -----------------------------------------------------------------------------
// GENEL AYARLAR (GENERAL SETTINGS)
// -----------------------------------------------------------------------------
listSec = input.string(defval='Liste 1', options=['Liste 1', 'Liste 2', 'Liste 3', 'Liste 4', 'Liste 5'], group='GENEL AYARLAR (GENERAL SETTINGS)', title='Hangi Listeyi Taramak İstiyorsunuz? (Which List Do You Want to Scan?)')
per = input.timeframe(defval='', title='Periyot (Timeframe)', group="GENEL AYARLAR (GENERAL SETTINGS)")

// Görünüm Ayarları (Appearance Settings)
bg_transparency = input.int(100, title="Arka Plan Şeffaflığı (Background Transparency)", minval=0, maxval=100, step=1, group="GÖRÜNÜM (APPEARANCE)")
text_transparency = input.int(0, title="Yazı Şeffaflığı (Text Transparency)", minval=0, maxval=100, step=1, group="GÖRÜNÜM (APPEARANCE)")
text_size_value = input.int(12, title="Yazı Boyutu (Text Size)", minval=8, maxval=24, step=1, group="GÖRÜNÜM (APPEARANCE)")

// Yazı boyutu dönüşümü (Text size conversion)
var string text_size = size.normal
if text_size_value <= 10
    text_size := size.tiny
else if text_size_value <= 12
    text_size := size.small
else if text_size_value <= 16
    text_size := size.normal
else if text_size_value <= 20
    text_size := size.large
else
    text_size := size.huge

// -----------------------------------------------------------------------------
// STRATEJİ AYARLARI (STRATEGY SETTINGS)
// -----------------------------------------------------------------------------

// Uyumsuzluk Ayarları (Divergence Settings)
lbR = input.int(1, title="Pivot Sağa Bakış (Pivot Lookback Right)", minval=1, group="UYUMSUZLUK AYARLARI (DIVERGENCE SETTINGS)")
lbL = input.int(5, title="Pivot Sola Bakış (Pivot Lookback Left)", minval=1, group="UYUMSUZLUK AYARLARI (DIVERGENCE SETTINGS)")
rangeUpper = input.int(30, title="Maksimum Aralık (Max of Lookback Range)", minval=1, group="UYUMSUZLUK AYARLARI (DIVERGENCE SETTINGS)")
rangeLower = input.int(1, title="Minimum Aralık (Min of Lookback Range)", minval=1, group="UYUMSUZLUK AYARLARI (DIVERGENCE SETTINGS)")

// RSI MA Kesişim Ayarları (RSI MA Crossover Settings)
rsiLength = input.int(14, minval=1, title="RSI Uzunluğu (RSI Length)", group="RSI MA KESİŞİM (RSI MA CROSSOVER)")
maType = input.string("SMA", "MA Türü (MA Type)", options=["SMA", "EMA", "SMMA (RMA)", "WMA", "VWMA"], group="RSI MA KESİŞİM (RSI MA CROSSOVER)")
maLength = input.int(14, minval=1, title="MA Uzunluğu (MA Length)", group="RSI MA KESİŞİM (RSI MA CROSSOVER)")

// -----------------------------------------------------------------------------
// HESAPLAMA FONKSİYONU (CALCULATION FUNCTION)
// -----------------------------------------------------------------------------

// Aralık Kontrolü (Range Check)
_inRange(cond) =>
    bars = ta.barssince(cond)
    rangeLower <= bars and bars <= rangeUpper

func() =>
    // --- 1. Uyumsuzluk (Divergence) ---
    rsiDivergence = ta.rsi(close, 14)
    osc = rsiDivergence

    plFound = ta.pivotlow(osc, lbL, lbR) != 0
    phFound = ta.pivothigh(osc, lbL, lbR) != 0

    // Pozitif Uyumsuzluk (Regular Bullish - PU)
    oscHL = osc[lbR] > ta.valuewhen(plFound, osc[lbR], 1) and _inRange(plFound[1])
    priceLL = low[lbR] < ta.valuewhen(plFound, low[lbR], 1)
    pu = priceLL and oscHL and plFound

    // Gizli Pozitif Uyumsuzluk (Hidden Bullish - GPU)
    oscLL = osc[lbR] < ta.valuewhen(plFound, osc[lbR], 1) and _inRange(plFound[1])
    priceHL = low[lbR] > ta.valuewhen(plFound, low[lbR], 1)
    gpu = priceHL and oscLL and plFound

    // Negatif Uyumsuzluk (Regular Bearish - NU)
    oscLH = osc[lbR] < ta.valuewhen(phFound, osc[lbR], 1) and _inRange(phFound[1])
    priceHH = high[lbR] > ta.valuewhen(phFound, high[lbR], 1)
    nu = priceHH and oscLH and phFound

    // Gizli Negatif Uyumsuzluk (Hidden Bearish - GNU)
    oscHH = osc[lbR] > ta.valuewhen(phFound, osc[lbR], 1) and _inRange(phFound[1])
    priceLH = high[lbR] < ta.valuewhen(phFound, high[lbR], 1)
    gnu = priceLH and oscHH and phFound

    // --- 2. RSI MA Kesişim (Crossover) ---
    rsiForMa = ta.rsi(close, rsiLength)
    ma = switch maType
        "SMA" => ta.sma(rsiForMa, maLength)
        "EMA" => ta.ema(rsiForMa, maLength)
        "SMMA (RMA)" => ta.rma(rsiForMa, maLength)
        "WMA" => ta.wma(rsiForMa, maLength)
        "VWMA" => ta.vwma(rsiForMa, maLength)

    rsiBuy = ta.crossover(rsiForMa, ma)
    rsiSell = ta.crossunder(rsiForMa, ma)

    [pu, gpu, nu, gnu, rsiBuy, rsiSell]

