//Function Entire Forecast
forecast(_type, _src, _flen, _lrlen) =>
    linreg_1 = ta.linreg(_src, _lrlen, _flen)
    _type == 'flat' ? _src : _type == 'linreg' ? linreg_1 : na
//-------------------------------------------------------------------------------------------------------------//


//-------------------------------------------------------------------------------------------------------------//
//Type MA
matype(_type, _src, _len) =>
    _len == 0 ? _src : _type == 'SMA' ? ta.sma(_src, _len) : _type == 'EMA' ? ta.ema(_src, _len) : _type == 'WMA' ? ta.wma(_src, _len) : _type == 'VWMA' ? ta.vwma(_src, _len) : _type == 'RMA' ? ta.rma(_src, _len) : _type == 'HMA' ? ta.hma(_src, _len) : na
//-------------------------------------------------------------------------------------------------------------//


// Function ADX Forecast
//-------------------------------------------------------------------------------------------------------------//
ADX_classic_up                             = ta.change (high)
ADX_classic_down                           = -ta.change(low)
ADX_classicADX_classic_plusDM              = na(ADX_classic_up)   ? na : ADX_classic_up   > ADX_classic_down and ADX_classic_up   > 0 ? ADX_classic_up   : 0
ADX_classicADX_classic_minusDM             = na(ADX_classic_down) ? na : ADX_classic_down > ADX_classic_up   and ADX_classic_down > 0 ? ADX_classic_down : 0
ADX_classic_truerange                      = ta.rma(ta.tr, ADX_len)
ADX_classic_plus                           = fixnan(100 * ta.rma(ADX_classicADX_classic_plusDM,  ADX_len) / ADX_classic_truerange)
ADX_classic_minus                          = fixnan(100 * ta.rma(ADX_classicADX_classic_minusDM, ADX_len) / ADX_classic_truerange)
ADX_classic_sum                            = ADX_classic_plus + ADX_classic_minus
ADX_Classic_                               = 100 * ta.rma(math.abs(ADX_classic_plus - ADX_classic_minus) / (ADX_classic_sum == 0 ? 1 : ADX_classic_sum), ADX_len)
