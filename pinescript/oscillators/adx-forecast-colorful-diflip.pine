ADX_Masa_SmoothedDirectionalMovementMinus  = 0.0

ADX_Masa_TrueRange                         = math.max(math.max(high - low, math.abs(high - nz(close[1]))), math.abs(low - nz(close[1])))
ADX_Masa_DirectionalMovementPlus           = high - nz(high[1]) > nz(low[1]) - low ? math.max(high - nz(high[1]), 0) : 0
ADX_Masa_DirectionalMovementMinus          = nz(low[1]) - low > high - nz(high[1]) ? math.max(nz(low[1]) - low, 0)   : 0

// Calculate Smoothed True Range, Smoothed +DM, and Smoothed -DM using rma
