indicator(title='Alloyz Traders_RSI by Sagar B', shorttitle='Alloyz Traders_RSI by Sagar B', format=format.price, precision=2, timeframe='')
len = input.int(9, minval=1, title='RSI')
src = input(close, 'Source')
_rsi = ta.rsi(src, len)
h_40 = plot(40, color=color.rgb(140, 173, 141), style=plot.style_line, linewidth=1, title='Line -40 ')
h_60 = plot(60, color=#db9696, style=plot.style_line, linewidth=1, title='Line -60 ')
p_rsi = plot(_rsi, color=color.new(color.white, 0), style=plot.style_line, linewidth=2, title='RSI - Plot')

wma_18 = ta.wma(_rsi, 18)

TEMA(series, length) =>
    if length > 0
        ema1 = ta.ema(series, length)
        ema2 = ta.ema(ema1, length)
        ema3 = ta.ema(ema2, length)
        (3 * ema1) - (3 * ema2) + ema3
    else
        na
_TEMA=TEMA(_rsi, 9)

plot(wma_18, color=color.new(color.red, 0), style=plot.style_line, linewidth=1, title='Strength')
plot(_TEMA, color=color.new(color.green, 0), style=plot.style_line, linewidth=1, title='Price')
