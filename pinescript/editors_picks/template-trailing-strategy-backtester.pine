          comment_profit = showAlertsEnabled ? msgAlertProfit : msgCommentProfit, comment_loss = showAlertsEnabled ? msgAlertLoss : msgCommentLoss,
          alert_profit = msgAlertProfit, alert_loss = msgAlertLoss)

