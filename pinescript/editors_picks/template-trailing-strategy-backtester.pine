exitCustLowerType = input.enum(defval = col.SeriesType.lowest, title = '‚ÄÉ‚ÄÉLower Type/Src‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÇ‚ÄÑ‚Äâ‚Ää‚Äã‚Äã‚Äã', inline = 'Lower Cust Exit', group = GROUP_EXIT, display = display.all, active = exitOrderType != OrderType.market and exitDistMethod == dr.DistMethod.cust)
