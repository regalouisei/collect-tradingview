  distMul = exitBiasEnabled ? input.float(defval = 5.0, title = '‚ÄÉ‚ÄÉBias Dist Mul‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚Äã‚Äã', step = 0.5, tooltip = 'The multiplier to define the exit bias distance offset.', group = GROUP_EXIT, display = display.data_window, active = exitOrderType != OrderType.market and exitBiasEnabled) : na)

