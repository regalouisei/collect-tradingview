if not isShortPosition
    if closedTradeExitIdIs(LONG_ORDER_ID_ENTRY) or closedTradeExitIdContains(SHORT_ORDER_ID_PREFIX)
        lastShortExitBarIndex := closedTradesExitBarIndex(1)
