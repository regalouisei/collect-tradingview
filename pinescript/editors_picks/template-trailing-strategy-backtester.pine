atrMethod = input.enum(defval = UpdateMethod.static, title = 'ATR Mtd', tooltip = 'The method to calculate the ATR used for the trailing. Use the ATR value at the time of open signal throughout the whole trade (STATIC), or use the current ATR value (DYNAMIC), or the ATR only of the green/red bars when try to enter/exit the trade (LADDER).', group = GROUP_VOLATILITY, display = display.data_window)
atrMaType = input.enum(defval = col.SeriesType.rma, title = '‚ÄÉ‚ÄÉATR Smooth Type/Len‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÑ', options = [col.SeriesType.alma, col.SeriesType.dema, col.SeriesType.ema, col.SeriesType.hma, col.SeriesType.rma, col.SeriesType.sma, col.SeriesType.swma, col.SeriesType.vwma, col.SeriesType.wma, col.SeriesType.highest, col.SeriesType.lowest], inline = 'ATR', group = GROUP_VOLATILITY, display = display.data_window)
atrLength = input.int(defval = 14, title = '‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã', minval = 1, tooltip = 'The smoothing type and the length to be used for the ATR calculation.', inline = 'ATR', group = GROUP_VOLATILITY, display = display.data_window)

enum StDevMode
    price = 'PRICE'
    ret = 'RETURN'

stdevMethod = input.enum(defval = UpdateMethod.static, title = 'StDev Mtd', tooltip = 'The method to calculate the standard deviation used for the trailing. Use the StDev value at the time of open signal throughout the whole trade (STATIC), or use the current StDev value (DYNAMIC), or the StDev only of the green/red bars when try to enter/exit the trade (LADDER).', group = GROUP_VOLATILITY, display = display.data_window)
stdevMode = input.enum(defval = StDevMode.price, title = '‚ÄÉ‚ÄÉStDev Mode', tooltip = 'Use the "PRICE" as source for the standard deviation calculations. Or use "RETURN" to use the log returns of the price as source for the standard deviation.', group = GROUP_VOLATILITY, display = display.data_window)
stdevPriceSrc = input.source(defval = close, title = '‚ÄÉ‚ÄÉStDev Src/Len‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚ÄÉ‚Äá‚Äâ', inline = 'StDev', group = GROUP_VOLATILITY, display = display.data_window)
stdevLength = input.int(defval = 20, title = '‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã‚Äã', minval = 1, tooltip = 'The source and the length to be used for the StDev calculation.', inline = 'StDev', group = GROUP_VOLATILITY, display = display.data_window)

// LOGIC ===============================================================================================================
// Calculate atr and StDev based on method selected, static atr when last open signal was emitted, dynamic is the current atr/stdev that change over time, ladder atr/stdev for positive and negative bars
var float ladderPositiveAtr = na
var float ladderNegativeAtr = na
var float ladderPositiveStDevs = na
var float ladderNegativeStDevs = na

getStandardOHLC() =>
    if chart.is_standard
        [open, high, low, close]
    else
        request.security(ticker.new(prefix = syminfo.prefix, ticker = syminfo.tickerid), '', [open, high, low, close])

[stOpen, stHigh, stLow, stClose] = getStandardOHLC()

getStandardTrueRange() =>
    na(stClose[1]) ? stHigh - stLow : math.max(stHigh - stLow, math.abs(stHigh - stClose[1]), math.abs(stLow - stClose[1]))

if atrMethod == UpdateMethod.ladder
    if stClose > stOpen
        ladderPositiveAtr := col.cust_series(atrLength, atrMaType, getStandardTrueRange()) // NOLINT this is intended so that only atr of green bars is calculated
    else
        ladderNegativeAtr := col.cust_series(atrLength, atrMaType, getStandardTrueRange()) // NOLINT this is intended so that only atr of red bars is calculated

if stdevMethod == UpdateMethod.ladder
    float stdevSrc = stdevMode == StDevMode.price ? stdevPriceSrc : math.log(stdevPriceSrc / stdevPriceSrc[1])
    if stClose > stOpen
        float positiveStDev = ta.stdev(stdevSrc, stdevLength, false) // NOLINT this is intended so that only deviation of green bars is calculated
        ladderPositiveStDevs := stdevMode == StDevMode.price ? positiveStDev : stdevPriceSrc * (math.exp(positiveStDev) - 1.0)
    else
        float negativeStDev = ta.stdev(stdevSrc, stdevLength, false) // NOLINT this is intended so that only deviation of red bars is calculated
        ladderNegativeStDevs := stdevMode == StDevMode.price ? negativeStDev : stdevPriceSrc * (math.exp(negativeStDev) - 1.0)

float dynamicAtr = col.cust_series(atrLength, atrMaType, getStandardTrueRange())
float staticAtr = ta.valuewhen(validOpenPosition, dynamicAtr, 0)
float dynamicStDev = ta.stdev(stdevPriceSrc, stdevLength)
float staticStDev = ta.valuewhen(validOpenPosition, dynamicStDev, 0)

float posAtr = switch atrMethod
    UpdateMethod.static => staticAtr
    UpdateMethod.dynamic => dynamicAtr
    UpdateMethod.ladder => ladderPositiveAtr

float negAtr = switch atrMethod
    UpdateMethod.static => staticAtr
    UpdateMethod.dynamic => dynamicAtr
    UpdateMethod.ladder => ladderNegativeAtr

float posStDev = switch stdevMethod
    UpdateMethod.static => staticStDev
    UpdateMethod.dynamic => dynamicStDev
    UpdateMethod.ladder => ladderPositiveStDevs

float negStDev = switch stdevMethod
    UpdateMethod.static => staticStDev
    UpdateMethod.dynamic => dynamicStDev
    UpdateMethod.ladder => ladderNegativeStDevs

modCount += 1

