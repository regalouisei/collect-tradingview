// LOGIC ===============================================================================================================
bool IS_TAKE_PROFIT_DIST_METHOD_CUST = takeProfitInitDistMethod == dr.DistMethod.cust
float takeProfitCustHigherPrice = IS_TAKE_PROFIT_DIST_METHOD_CUST ? math.round_to_mintick(col.cust_series(longTakeProfitInitDistMul, takeProfitCustHigherType, takeProfitCustHigherSrc)) : na
// Calculate the actual long take profit price and ticks that is higher from the reference using the distance
getLongTakeProfitPrice(dr.DistSettings dist, float reference, dr.AuxData shortBiasedAuxData, dr.RestrictSettings restrict, dr.DistSettings bias = na) =>
    dist.higher_short_biased_price(reference, shortBiasedAuxData, restrict, bias, takeProfitCustHigherPrice)

getlongTakeProfitTicks(dr.DistSettings distSettings, float referencePrice, dr.AuxData shortBiasedAuxData) =>
    distSettings.ticks(referencePrice, shortBiasedAuxData)

bool TAKE_PROFIT_ADJUSTMENT_IS_NEEDED = takeProfitAdjustmentEnabled ? not IS_TAKE_PROFIT_DIST_METHOD_CUST : false

if longLimitOrStopEntryIsActive or validOpenLongPosition
    float currentLongTakeProfitPrice = longEntryPrice
    currentLongTakeProfitShortBiasedAuxData = dr.AuxData.new(atr = negAtr, stdev = negStDev, stopLossDist = longStopLossDist)
    dr.DistSettings currentLongTakeProfitDistSettings = longTakeProfitInitDistSettings
    dr.RestrictSettings currentTakeProfitRestrictSettings = takeProfitInitRestrictSettings
    dr.DistSettings currentTakeProfitBiasSettings = takeProfitInitBiasSettings
    for i = 0 to TAKE_PROFIT_TARGETS_SIZE
        currentLongTakeProfitShortBiasedAuxData.baseQuantity := longTakeProfitQuantities.get(i)
        currentLongTakeProfitPrice := getLongTakeProfitPrice(currentLongTakeProfitDistSettings, currentLongTakeProfitPrice, currentLongTakeProfitShortBiasedAuxData, currentTakeProfitRestrictSettings, currentTakeProfitBiasSettings)
        longTakeProfitPrices.set(i, currentLongTakeProfitPrice)
        longTrailTakeProfitOffsetTicks.set(i, getlongTakeProfitTicks(takeProfitTrailDistSettings, currentLongTakeProfitPrice, currentLongTakeProfitShortBiasedAuxData))
        if i == 0
            currentLongTakeProfitDistSettings := longTakeProfitStepDistSettings
            currentTakeProfitRestrictSettings := takeProfitStepRestrictSettings
            currentTakeProfitBiasSettings := takeProfitStepBiasSettings
else if enteredLongTrade and TAKE_PROFIT_ADJUSTMENT_IS_NEEDED
    float longEntryPriceOffset = longEntryPrice - nz(longEntryPrice[1], longEntryPrice)
    for i = 0 to TAKE_PROFIT_TARGETS_SIZE
        longTakeProfitPrices.set(i, longTakeProfitPrices.get(i) + longEntryPriceOffset)
        longTrailTakeProfitOffsetTicks.set(i, longTrailTakeProfitOffsetTicks.get(i) + math.floor(longEntryPriceOffset / syminfo.mintick))

float takeProfitCustLowerPrice = IS_TAKE_PROFIT_DIST_METHOD_CUST ? math.round_to_mintick(col.cust_series(shortTakeProfitInitDistMul, takeProfitCustLowerType, takeProfitCustLowerSrc)) : na
// Calculate the actual short take price and ticks that is lower from the reference using the distance
getShortTakeProfitPrice(dr.DistSettings dist, float reference, dr.AuxData longBiasedAuxData, dr.RestrictSettings restrict, dr.DistSettings bias = na) =>
    dist.lower_long_biased_price(reference, longBiasedAuxData, restrict, bias, takeProfitCustLowerPrice)

getShortTakeProfitTicks(dr.DistSettings distSettings, float referencePrice, dr.AuxData longBiasedAuxData) =>
    distSettings.ticks(referencePrice, longBiasedAuxData)

if shortLimitOrStopEntryIsActive or validOpenShortPosition
    float currentShortTakeProfitPrice = shortEntryPrice
    currentShortTakeProfitLongBiasedAuxData = dr.AuxData.new(atr = posAtr, stdev = posStDev, stopLossDist = shortStopLossDist)
    dr.DistSettings currentShortTakeProfitDistSettings = shortTakeProfitInitDistSettings
    dr.RestrictSettings currentTakeProfitRestrictSettings = takeProfitInitRestrictSettings
    dr.DistSettings currentTakeProfitBiasSettings = takeProfitInitBiasSettings
    for i = 0 to TAKE_PROFIT_TARGETS_SIZE
        currentShortTakeProfitLongBiasedAuxData.baseQuantity := shortTakeProfitQuantities.get(i)
        currentShortTakeProfitPrice := getShortTakeProfitPrice(currentShortTakeProfitDistSettings, currentShortTakeProfitPrice, currentShortTakeProfitLongBiasedAuxData, currentTakeProfitRestrictSettings, currentTakeProfitBiasSettings)
        shortTakeProfitPrices.set(i, currentShortTakeProfitPrice)
        shortTrailTakeProfitOffsetTicks.set(i, getShortTakeProfitTicks(takeProfitTrailDistSettings, currentShortTakeProfitPrice, currentShortTakeProfitLongBiasedAuxData))
        if i == 0
            currentShortTakeProfitDistSettings := shortTakeProfitStepDistSettings
            currentTakeProfitRestrictSettings := takeProfitStepRestrictSettings
            currentTakeProfitBiasSettings := takeProfitStepBiasSettings
else if enteredShortTrade and TAKE_PROFIT_ADJUSTMENT_IS_NEEDED
    float shortEntryPriceOffset = shortEntryPrice - nz(shortEntryPrice[1], shortEntryPrice)
    for i = 0 to TAKE_PROFIT_TARGETS_SIZE
        shortTakeProfitPrices.set(i, shortTakeProfitPrices.get(i) + shortEntryPriceOffset)
        shortTrailTakeProfitOffsetTicks.set(i, shortTrailTakeProfitOffsetTicks.get(i) + math.floor(shortEntryPriceOffset / syminfo.mintick))

modCount += 1

